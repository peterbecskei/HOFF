
{
  "manifest_version": 3,
  "name": "Extract link",
  "version": "1.0.0",
  "description": "Extracts links.",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "sidePanel", "sidePanel", "storage", "contextMenus", "activeTab", "tabs",
    "alarms",
    "notifications",
    "storage",
    "sidePanel"]  ,
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
   "action": {
    "default_icon": {
      "16": "icons/icon.png",
      "48": "icons/icon1.png",
      "128": "icons/icon2.png"
    },
      "default_popup": "popup.html",
    "default_title": "Tab HTML Saver"

  },
    "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon1.png",
    "128": "icons/icon2.png"
  },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
 "web_accessible_resources": [{
    "resources": ["script.js"],
    "matches": ["<all_urls>"]
  }]
}

